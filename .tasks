[project-run]
command=poetry run python app.py 200
cwd=<root>
output=terminal

[project-update]
command=poetry update
cwd=<root>
output=terminal

[project-test]
command=poetry run pytest
cwd=<root>
output=terminal

[project-profile]
command=poetry run python -m cProfile -s cumtime app.py 200
cwd=<root>
output=terminal
